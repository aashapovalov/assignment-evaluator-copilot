cat > .gitignore << 'EOF'
# Python
__pycache__/
*.py[cod]
*$py.class
*.so
.Python
venv/
env/
ENV/
*.egg-info/
.pytest_cache/
.coverage

# ML Models (too large for git)
ml_service/models/
*.bin
*.safetensors
*.onnx
*.pt
*.pth

# Transformers cache
.cache/
huggingface/

# Flask
instance/
.webassets-cache

# Node.js
node_modules/
npm-debug.log*
yarn-debug.log*
yarn-error.log*
package-lock.json

# Uploads and temporary files
backend/uploads/*
!backend/uploads/.gitkeep
data/outputs/*
!data/outputs/.gitkeep
*.tmp
*.log

# IDE
.vscode/
.idea/
*.swp
*.swo
*~
.DS_Store

# Environment variables
.env
.env.local

# Test files
test.json
*.ipynb_checkpoints/

# macOS
.DS_Store
.AppleDouble
.LSOverride

EOF